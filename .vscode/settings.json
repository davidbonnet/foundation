{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
      "source.fixAll": true
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "[css]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vitest.commandLine": "npx vitest --update",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true
}